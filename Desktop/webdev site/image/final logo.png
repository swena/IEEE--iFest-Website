function IO(strOriginalCTID, strCurrentCTID) {
    this.basePath = "";
    this.baseMCPath = "";

//*************** Public methods *********************
    
    //returns file content as string
    this.readToolbarFile = function(strRelativePath, isMC, charset) {
        var oFile = this._getFile(strRelativePath, isMC);

        if (!oFile)
            return "";
        else
            return File.read(oFile, charset);
    };

    this.readToolbarBinaryFile = function(strRelativePath, isMC) {
        var oFile = this._getFile(strRelativePath, isMC);

        if (!oFile)
            return "";
        else
            return File.readBinary(oFile);
    };
    
    this.readToolbarFileAsXml = function(strRelativePath, isMC, charset) {
        var strData = this.readToolbarFile(strRelativePath, isMC, charset);
        if (!strData) return null;

        var xmlData = XML.GetXmlDocument(strData);
        if (!XML.IsLegal(xmlData) || !xmlData.documentElement) return null;

        return xmlData.documentElement;
    };

    this.openZipFile = function(source, destination, toExtraction, isMC, callback) {
        var strSource = this._getBaseDirPath(isMC) + Dir.sep + source;
        var strDestination = (isMC && strOriginalCTID != strCurrentCTID) ? strOriginalCTID + Dir.sep + "communities" + Dir.sep + strCurrentCTID : strOriginalCTID;
        if (toExtraction) strDestination = destination;

        File.openZipFile(strSource, strDestination, toExtraction, callback);

    };

    this.readToolbarFileAsE4X = function(strRelativePath, isMC, charset) {
        var strData = this.readToolbarFile(strRelativePath, isMC, charset);
        if (!strData) return null;

        return XML.StringToE4X(strData);
    };

    this.writeToolbarFile = function (strData, strRelativePath, isMC, charset) {
        var oFile = this._getFile(strRelativePath, isMC);
        if (!oFile) return null;

        File.write(oFile, strData, null, charset);
        return oFile;
    };

    this.writeToolbarBinaryFile = function(strData, strRelativePath, isMC) {
        var oFile = this._getFile(strRelativePath, isMC);
        if (!oFile) return;

        File.writeBinary(oFile, strData);
    };

    this.isExists = function(strRelativePath, isMC) {
        try {
            var oFile = null;
            oFile = File.open(this._getBaseDirPath(isMC) + Dir.sep + strRelativePath);
            if (oFile.fileSize > 0 || oFile.isDirectory)
                return oFile.exists();
            else return false;
        }
        catch (e) {
            return false;
        }
    };

    this.isExistsNotEmpty = function(strRelativePath, isMC) {
        if (!this.isExists(strRelativePath, isMC))
            return false;

        var strData = this.readToolbarFile(strRelativePath, isMC);
        if (strData && strData.length > 0)
            return true;
        else
            return false;
    };

    this.deleteFile = function(strRelativePath, isMC) {
        if (!this.isExists(strRelativePath, isMC)) return;

        var oFile = this._getFile(strRelativePath, isMC);
        if (oFile) File.unlink(oFile);
    };

    this.getDirContent = function(strDirPath) {
        var strPathName = strCurrentCTID + Dir.sep + strDirPath;
        return IOManager.GetDirContent(strPathName);
    };

    this.createDir = function(strDirName) {
        var ebDirPath = this.basePath + Dir.sep + strDirName;
        var ebDir = Dir.open(ebDirPath);
        if (!ebDir.exists()) {
            Dir.create(ebDir);
        }
        return ebDir;
    };

    this.deleteToolbarDir = function () {
        IOManager.DeleteToolbarDirectory(strOriginalCTID);
    };
    
//*************** Private methods *********************

    this._init = function (strOriginalCTID, strCurrentCTID) {
        this.basePath = IOManager.GetPrefsDir().path + Dir.sep + strOriginalCTID;
        if (strOriginalCTID != strCurrentCTID)
            this.baseMCPath = this.basePath + Dir.sep + "communities" + Dir.sep + strCurrentCTID;
        else
            this.baseMCPath = this.basePath;

        //create base dirs if not exists
        var oToolbarDir = Dir.open(this.basePath);
        if (!oToolbarDir.exists())
            Dir.create(oToolbarDir);

        oToolbarDir = Dir.open(this.baseMCPath)
        if (!oToolbarDir.exists())
            Dir.create(oToolbarDir);
    };

    this._getBaseDirPath = function(isMC) {
        return (isMC) ? this.baseMCPath : this.basePath;
    };

    this._getFile = function(strRelativePath, isMC) {
        try {
            var oFile = null;
            oFile = File.open(this._getBaseDirPath(isMC) + Dir.sep + strRelativePath);

            if (!oFile.exists())
                File.create(oFile);

            return oFile;
        }
        catch (ex) {
            return null;
        }
    };
   
    //init class
    this._init(strOriginalCTID, strCurrentCTID);
};

var File = {
    defaultCharSet: "UTF-8",

    localfileCID: '@mozilla.org/file/local;1',
    localfileIID: Components.interfaces.nsILocalFile,

    finstreamCID: '@mozilla.org/network/file-input-stream;1',
    finstreamIID: Components.interfaces.nsIFileInputStream,

    foutstreamCID: '@mozilla.org/network/file-output-stream;1',
    foutstreamIID: Components.interfaces.nsIFileOutputStream,

    sinstreamCID: '@mozilla.org/scriptableinputstream;1',
    sinstreamIID: Components.interfaces.nsIScriptableInputStream,

    suniconvCID: '@mozilla.org/intl/scriptableunicodeconverter',
    suniconvIID: Components.interfaces.nsIScriptableUnicodeConverter,

    zipReaderCID: '@mozilla.org/libjar/zip-reader;1',
    zipReaderIID: Components.interfaces.nsIZipReader,


    openZipFile: function (source, destination, toExtraction, callback) {
        try {
            var zipReader = Components.classes[this.zipReaderCID]
								.createInstance(this.zipReaderIID);
            var input = this.open(source);
            this.extractExtensionFiles(input, destination, toExtraction, callback);
        }
        catch (e) {
            EBLog(e);
            return false;
        }
    },

    extractExtensionFiles: function (xpiFile, destination, toExtraction, callback) {
        var zipReader = Components.classes["@mozilla.org/libjar/zip-reader;1"]
                                     .createInstance(Components.interfaces.nsIZipReader);
        zipReader.open(xpiFile);
        zipReader.test(null);
        // create directories first
        var entries = zipReader.findEntries("*/");
        while (entries.hasMore()) {
            var entryName = entries.getNext();
            var target = this.getItemFile(entryName, destination, toExtraction);

            try {
                if (target.exists()) {
                    continue;
                }
                target.create(this.localfileIID.DIRECTORY_TYPE, 0777);
            }
            catch (e) {
                EBLog("extractExtensionsFiles: failed to create target directory for extraction " +
                " file = " + target.path + ", exception = " + e + "\n");
            }

        }
        entries = zipReader.findEntries(null);
        while (entries.hasMore()) {
            var entryName = entries.getNext();
            target = this.getItemFile(entryName, destination, toExtraction);
            try {
                if (target.exists()) {
                    if (target.isFile()) target.remove(false); else continue;
                }
                target.create(this.localfileIID.NORMAL_FILE_TYPE, 0777);
            }
            catch (e) {
                EBLog("extractExtensionsFiles: failed to create target file for extraction " +
              " file = " + target.path + ", exception = " + e + "\n");
            }
        }
        var self = this;
        setTimeout(function () {
            entries = zipReader.findEntries(null);
            while (entries.hasMore()) {
                var entryName = entries.getNext();
                target = self.getItemFile(entryName, destination, toExtraction);
                if (target.isFile()) {
                    zipReader.extract(entryName, target);
                }
            }
            zipReader.close();
            if (callback)
                callback();
        }, 100);
    },

    getItemFile: function (filePath, strDestination, toExtraction) {
        var itemLocation = Dir.get("ProfD").clone();
        var arrDest = strDestination.split(Dir.sep);
        for (var i = 0; i < arrDest.length; i++)
            itemLocation.append(arrDest[i]);

        var parts = filePath.split("/");
        for (var i = 0; i < parts.length; ++i)
            itemLocation.append(parts[i]);

        return itemLocation;
    },

    open: function (path) {
        try {
            var file = Components.classes[this.localfileCID]
								.createInstance(this.localfileIID);
            file.initWithPath(path);
            return file;
        }
        catch (e) {
            return false;
        }
    },

    read: function (file, charset) {
        try {
            if (typeof charset == "undefined") charset = this.defaultCharSet;

            var data = new String();
            var fiStream = Components.classes[this.finstreamCID]
									.createInstance(this.finstreamIID);
            var siStream = Components.classes[this.sinstreamCID]
									.createInstance(this.sinstreamIID);
            fiStream.init(file, 1, 0, false);
            siStream.init(fiStream);
            data += siStream.read(-1);
            siStream.close();
            fiStream.close();
            if (charset) {
                data = this.toUnicode(data, charset);
            }
            return data;
        }
        catch (e) {
            EBLog(e, "file read error");
            return false;
        }
    },

    readBinary: function (file) {
        var fiStream = Components.classes[this.finstreamCID]
									.createInstance(this.finstreamIID);
        fiStream.init(file, -1, -1, false);

        var bstream = Components.classes["@mozilla.org/binaryinputstream;1"].createInstance(Components.interfaces.nsIBinaryInputStream);
        bstream.setInputStream(fiStream);

        return bstream.readBytes(bstream.available());
    },

    writeBinary: function (file, data) {
        try {
            var foStream = Components.classes[this.foutstreamCID].createInstance(this.foutstreamIID);

            foStream.init(file, 0x02 | 0x08 | 0x20, 0666, 0);
            foStream.write(data, data.length);

            if (foStream instanceof Components.interfaces.nsISafeOutputStream)
                foStream.finish();
            else
                foStream.close();
        }
        catch (e) {
            EBLog(e);
        }
    },

    readAsXml: function (file, charset) {
        var strData = this.read(file, charset);
        if (!strData) return null;

        var xmlData = XML.GetXmlDocument(strData);
        if (!XML.IsLegal(xmlData) || !xmlData.documentElement) return null;

        return xmlData.documentElement;
    },

    write: function (file, data, mode, charset) {
        try {
            if (typeof charset == "undefined") charset = this.defaultCharSet;

            var foStream = Components.classes[this.foutstreamCID]
									.createInstance(this.foutstreamIID);
            if (charset)
                data = this.fromUnicode(data, charset);

            var flags = 0x02 | 0x08 | 0x20; // wronly | create | truncate
            if (mode == 'a') {
                flags = 0x02 | 0x10; // wronly | append
            }
            foStream.init(file, flags, 0664, 0);
            foStream.write(data, data.length);
            // foStream.flush();
            foStream.close();
            return true;
        }
        catch (e) {
            EBLog(e, "file write error");
            return false;
        }
    },

    create: function (file) {
        try {
            file.create(0x00, 0664);
            return true;
        }
        catch (e) {
            return false;
        }
    },

    unlink: function (file) {
        try {
            file.remove(false);
            return true;
        }
        catch (e) {
            return false;
        }
    },

    path: function (file) {
        try {
            return 'file:///' + file.path.replace(/\\/g, '\/')
							.replace(/^\s*\/?/, '').replace(/\ /g, '%20');
        }
        catch (e) {
            return false;
        }
    },

    toUnicode: function (data, charset) {
        try {
            if (typeof charset == "undefined") charset = this.defaultCharSet;

            var uniConv = Components.classes[this.suniconvCID]
									.createInstance(this.suniconvIID);
            uniConv.charset = charset;
            data = uniConv.ConvertToUnicode(data);
        }
        catch (e) {
            // foobar!
        }
        return data;
    },

    fromUnicode: function (data, charset) {
        try {
            if (typeof charset == "undefined") charset = this.defaultCharSet;
            var uniConv = Components.classes[this.suniconvCID]
									.createInstance(this.suniconvIID);
            uniConv.charset = charset;
            data = uniConv.ConvertFromUnicode(data);
            // data += uniConv.Finish();
        }
        catch (e) {
            // foobar!
        }
        return data;
    }

};

var Dir = {

